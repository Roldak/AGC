with GC;
with GC.Storage;
with System;
with Ada.Unchecked_Conversion;
procedure Test is
   AGC_Base_Root_Count : Natural := GC.Root_Count;
   pragma Default_Storage_Pool (GC.Storage.Pool);
   type T is record
      X : Integer;
   end record;
   procedure AGC_Visit_Test_T (X : System.Address);
   procedure AGC_Visit_Test_T (X : System.Address) is
      pragma Suppress (Accessibility_Check);
      type Rec_Access is access all T;
      for Rec_Access'Size use Standard'Address_Size;
      function Conv is new Ada.Unchecked_Conversion
        (System.Address, Rec_Access);
      R : aliased T := Conv (X).all;
   begin
      null;
   end AGC_Visit_Test_T;
   type T_Access is access all T;
   procedure AGC_Visit_Test_T_Access is new GC.Visit_Access_Type
     (T, T_Access, AGC_Visit_Test_T);
   type U is record
      A : T_Access;
   end record;
   procedure AGC_Visit_Test_U (X : System.Address);
   procedure AGC_Visit_Test_U (X : System.Address) is
      pragma Suppress (Accessibility_Check);
      type Rec_Access is access all U;
      for Rec_Access'Size use Standard'Address_Size;
      function Conv is new Ada.Unchecked_Conversion
        (System.Address, Rec_Access);
      R : aliased U := Conv (X).all;
   begin
      declare
         C : aliased Test.T_Access := R.A;
      begin
         AGC_Visit_Test_T_Access (C'Address);
      end;
   end AGC_Visit_Test_U;
   type U_Access is access all U;
   procedure AGC_Visit_Test_U_Access is new GC.Visit_Access_Type
     (U, U_Access, AGC_Visit_Test_U);
   function AGC_Func_0 return U_Access is
      AGC_Base_Root_Count : Natural               := GC.Root_Count;
      AGC_Temp_0          : aliased Test.T_Access := new T'(X => 1);
   begin
      GC.Push_Root (AGC_Temp_0'Address, AGC_Visit_Test_T_Access'Address);
      return AGC_Ret : U_Access := new U'(A => AGC_Temp_0) do
         GC.Pop_Roots (AGC_Base_Root_Count);
      end return;
   end AGC_Func_0;
   X : aliased U_Access := AGC_Func_0;
begin
   GC.Push_Root (X'Address, AGC_Visit_Test_U_Access'Address);
   GC.Collect;
   X.A.X := 2;
   GC.Pop_Roots (AGC_Base_Root_Count);
end Test;
