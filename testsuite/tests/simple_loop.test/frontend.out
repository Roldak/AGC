with GC;
with Ada.Text_IO; use Ada.Text_IO;
procedure Test is
   type Integer_Access is access all Integer;
   X : aliased Integer_Access := null;
begin
   GC.Push_Reachable (X'Address);
   for I in 1 .. 10_000 loop
      X := GC.Temp (0, GC.Register (new Integer'(42)));
      GC.Untemp (0);
   end loop;
   GC.Pop_Reachable (1);
end Test;
