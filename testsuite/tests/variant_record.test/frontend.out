with AGC;
with AGC.Storage;
with System;
with Ada.Unchecked_Conversion;
procedure Test is
   AGC_Base_Root_Count : Natural := AGC.Root_Count;
   pragma Default_Storage_Pool (AGC.Storage.Pool);
   type Integer_Access is access all Integer;
   procedure AGC_Visit_Test_Integer_Access is new AGC.Visit_Access_Type
     (Integer, Integer_Access, AGC.No_Op);
   type Kind is (A, B, C);
   type Rec (K : Kind) is record
      X : Integer;
      case K is
         when A =>
            null;
         when B =>
            Y : Integer;
         when C =>
            Z : Integer_Access;
      end case;
   end record;
   procedure AGC_Visit_Test_Rec (X : System.Address);
   procedure AGC_Visit_Test_Rec (X : System.Address) is
      pragma Suppress (Accessibility_Check);
      type Rec_Access is access all Rec;
      for Rec_Access'Size use Standard'Address_Size;
      function Conv is new Ada.Unchecked_Conversion
        (System.Address, Rec_Access);
      R : aliased Rec := Conv (X).all;
   begin
      case R.K is
         when A =>
            null;
         when B =>
            null;
         when C =>
            declare
               C : aliased Test.Integer_Access := R.Z;
            begin
               AGC_Visit_Test_Integer_Access (C'Address);
            end;
      end case;
   end AGC_Visit_Test_Rec;
   R_1 : aliased Rec := (A, 1);
   function AGC_Func_0 return Rec is
      AGC_Base_Root_Count : Natural                     := AGC.Root_Count;
      AGC_Temp_0          : aliased Test.Integer_Access := new Integer'(3);
   begin
      AGC.Push_Root
        (AGC_Temp_0'Address, AGC_Visit_Test_Integer_Access'Address);
      return AGC_Ret : Rec := (C, 2, AGC_Temp_0) do
         AGC.Pop_Roots (AGC_Base_Root_Count);
      end return;
   end AGC_Func_0;
begin
   AGC.Push_Root (R_1'Address, AGC_Visit_Test_Rec'Address);
   declare
      R_2 : aliased Rec := AGC_Func_0;
   begin
      AGC.Push_Root (R_2'Address, AGC_Visit_Test_Rec'Address);
      AGC.Collect;
   end;
   AGC.Pop_Roots (AGC_Base_Root_Count);
end Test;
