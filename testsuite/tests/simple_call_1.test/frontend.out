with GC;
with Ada.Text_IO; use Ada.Text_IO;
procedure Test is
   type Integer_Access is access all Integer;
   procedure Inner (X : Integer_Access) is
      Y : aliased Integer_Access := GC.Temp (0, GC.Register (new Integer'(2)));
   begin
      GC.Untemp (0);
      GC.Push_Reachable (Y'Address);
      null;
      GC.Pop_Reachable (1);
   end Inner;
   A : aliased Integer_Access := GC.Temp (1, GC.Register (new Integer'(1)));
begin
   GC.Untemp (1);
   GC.Push_Reachable (A'Address);
   Inner (A);
   GC.Pop_Reachable (1);
end Test;
