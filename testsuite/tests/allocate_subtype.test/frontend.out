with AGC;
with AGC.Standard;
with AGC.Storage.Get;
with System;
with Ada.Unchecked_Conversion;
procedure Test is
   AGC_Base_Root_Count : Natural := AGC.Root_Count;
   pragma Default_Storage_Pool (AGC.Storage.Get.Pool);
   subtype My_Natural is Integer range 0 .. Integer'Last;
   type My_Nat_Access is access My_Natural;
   procedure AGC_Visit_My_Nat_Access is new AGC.Visit_Access_Type
     (My_Natural, My_Nat_Access, False, AGC.No_Op);
   X : aliased My_Nat_Access := new My_Natural'(2);
begin
   AGC.Push_Root (X'Address, Test.AGC_Visit_My_Nat_Access'Address);
   AGC.Collect;
   AGC.Pop_Roots (AGC_Base_Root_Count);
end Test;
