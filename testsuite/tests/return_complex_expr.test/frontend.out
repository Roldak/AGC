with AGC;
with AGC.Storage.Get;
with System;
with Ada.Unchecked_Conversion;
with Ada.Text_IO; use Ada.Text_IO;
procedure Test is
   AGC_Base_Root_Count : Natural := AGC.Root_Count;
   pragma Default_Storage_Pool (AGC.Storage.Get.Pool);
   type Integer_Access is access all Integer;
   procedure AGC_Visit_Test_Integer_Access is new AGC.Visit_Access_Type
     (Integer, Integer_Access, AGC.No_Op);
   function Inner (X : Integer) return Integer_Access is
      AGC_Base_Root_Count : Natural := AGC.Root_Count;
   begin
      return AGC_Ret : Integer_Access := new Integer'(X) do
         AGC.Pop_Roots (AGC_Base_Root_Count);
      end return;
   end Inner;
   function Add (A, B, C : Integer) return Integer is
      AGC_Base_Root_Count : Natural := AGC.Root_Count;
   begin
      declare
         AGC_Temp_2 : aliased Test.Integer_Access := Inner (C);
      begin
         AGC.Push_Root
           (AGC_Temp_2'Address, AGC_Visit_Test_Integer_Access'Address);
         declare
            AGC_Temp_1 : aliased Test.Integer_Access := Inner (B);
         begin
            AGC.Push_Root
              (AGC_Temp_1'Address, AGC_Visit_Test_Integer_Access'Address);
            declare
               AGC_Temp_0 : aliased Test.Integer_Access := Inner (A);
            begin
               AGC.Push_Root
                 (AGC_Temp_0'Address, AGC_Visit_Test_Integer_Access'Address);
               return
                 AGC_Ret : Integer :=
                   AGC_Temp_0.all + AGC_Temp_1.all + AGC_Temp_2.all do
                  AGC.Pop_Roots (AGC_Base_Root_Count);
               end return;
            end;
         end;
      end;
   end Add;
   A : Integer := Add (1, 2, 3);
begin
   AGC.Collect;
   AGC.Pop_Roots (AGC_Base_Root_Count);
end Test;
